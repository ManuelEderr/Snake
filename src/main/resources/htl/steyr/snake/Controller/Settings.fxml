      <Label layoutX="101.0" layoutY="90.0" text="Difficulty" />
      <Label layoutX="451.0" layoutY="90.0" text="Music" />
      <ChoiceBox fx:id="chooseDifficulty" layoutX="50.0" layoutY="119.0" prefWidth="150.0" value="normal" />
