      <ChoiceBox fx:id="chooseBarriers" layoutX="50.0" layoutY="228.0" prefWidth="150.0" value="0" />
      <Label layoutX="111.0" layoutY="200.0" text="Barriers" />
      <Label layoutX="453.0" layoutY="200.0" text="Name" />
      <TextField fx:id="nameTextField" layoutX="395.0" layoutY="228.0" promptText="Name" text="Name" />
   </children>
</AnchorPane>
